{
    "name": "rithis/page-bundle",
    "description": "Простой бандл для работы со статическими страницами",
    "type": "symfony-bundle",
    "keywords": ["symfony2", "bundle", "page"],
    "homepage": "https://github.com/rithis/RithisPageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vyacheslav Slinko",
            "email": "vyacheslav.slinko@rithis.com",
            "role": "Founder"
        }
    ],
    "support": {
        "email": "support@rithis.com",
        "issues": "https://github.com/rithis/RithisPageBundle/issues",
        "wiki": "https://github.com/rithis/RithisPageBundle/wiki",
        "source": "https://github.com/rithis/RithisPageBundle"
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "2.1.*",
        "doctrine/orm": "2.3.*",
        "doctrine/doctrine-bundle": "1.0.*",
        "jms/di-extra-bundle": "@dev",
        "stof/doctrine-extensions-bundle": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "behat/symfony2-extension": "*",
        "behat/common-contexts": "@dev",
        "knplabs/knp-menu-bundle": "@dev"
    },
    "suggest": {
        "knplabs/knp-menu-bundle": "Позволяет генеровать меню содержащее страницы с определенными метками"
    },
    "autoload": {
        "psr-0": {
            "Rithis\\PageBundle": ""
        }
    },
    "target-dir": "Rithis/PageBundle"
}
